&lt;p&gt;Test :&lt;/p&gt;
&lt;div class="sql-block" id="sql-0"&gt;
 &lt;textarea id="sql-0-code"&gt;CREATE TABLE etudiants (
 id INTEGER PRIMARY KEY,
 nom TEXT,
 age INTEGER
);

INSERT INTO etudiants VALUES (1, &amp;#39;Alice&amp;#39;, 22), (2, &amp;#39;Bob&amp;#39;, 25);&lt;/textarea&gt;
 &lt;div class="sql-toolbar"&gt;
 &lt;button id="sql-0-run" onclick="sqlRun('sql-0')"&gt;&amp;#9654; Exécuter&lt;/button&gt;
 &lt;button id="sql-0-reset" onclick="sqlReset('sql-0')"&gt;&amp;#8634; Réinitialiser&lt;/button&gt;
 &lt;span class="sql-status" id="sql-0-status"&gt;&lt;/span&gt;
 &lt;/div&gt;
 &lt;div class="sql-output sql-output-hidden" id="sql-0-output"&gt;&lt;/div&gt;
&lt;/div&gt;
&lt;div class="sql-block" id="sql-1"&gt;
 &lt;textarea id="sql-1-code"&gt;
select * from etudiants;
&lt;/textarea&gt;
 &lt;div class="sql-toolbar"&gt;
 &lt;button id="sql-1-run" onclick="sqlRun('sql-1')"&gt;&amp;#9654; Exécuter&lt;/button&gt;
